  "main": "/src/TableTrHref.js",
  "directories": {
    "example": "examples",
    "test": "test"
  },
  "scripts": {
    "test": "nyc --reporter=text --reporter=html mocha",
    "dev": "webpack --mode=development",
    "watch": "webpack --mode=development --watch",
    "build": "webpack --mode=production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mp3000mp/table-tr-href.git"
  },
  "keywords": [
    "table",
    "tr",
    "td",
    "a",
    "href"
  ],
  "author": "mp3000mp",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mp3000mp/table-tr-href/issues"
  },
  "homepage": "https://github.com/mp3000mp/table-tr-href#readme",
  "files": [
    "dist/*.css",
    "dist/*.js"
  ],
  "devDependencies": {
    "autoprefixer": "^9.6.1",
    "cheerio": "^1.0.0-rc.3",
    "css-loader": "^3.2.0",
    "eslint": "^6.4.0",
    "eslint-loader": "^3.0.2",
    "jsdom": "^15.1.1",
    "jsdom-global": "^3.0.2",
    "mocha": "^6.2.0",
    "node-sass": "^4.12.0",
    "nyc": "^14.1.1",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0"
  }
